                <Font name="Century Gothic Bold" size="22.0" />
            </font>
         <padding>
            <Insets left="-50.0" />
         </padding>
        </JFXButton>
        <JFXButton fx:id="openPasswordGenerator" layoutX="383.0" layoutY="158.0" onAction="#openPasswordGeneratorMenu" prefHeight="75.0" prefWidth="212.0" text="Create Password" textFill="WHITE">
            <font>
                <Font name="Century Gothic Bold" size="23.0" />
            </font>
         <padding>
            <Insets left="-10.0" />
         </padding>
        </JFXButton>
      <JFXButton fx:id="closeButtonMainMenu" layoutX="507.0" onAction="#closeButtomMainMenuAction" prefHeight="45.0" prefWidth="96.0" text="Close" textFill="WHITE">
