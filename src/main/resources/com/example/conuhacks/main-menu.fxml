<AnchorPane fx:id="anchorPaneMenu" prefHeight="315.0" prefWidth="400.0" styleClass="backgroundMenu" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.conuhacks.HelloController">
    <children>
      <ImageView fitHeight="316.0" fitWidth="373.0" layoutX="23.0" pickOnBounds="true" preserveRatio="true">
